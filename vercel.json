{
  "version": 2,
  "name": "nuestro-pulso-app",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/.*",
      "dest": "/index.html"
    }
  ],
  "env": {
    "NODE_VERSION": "18.17.0"
  },
  "functions": {},
  "regions": ["iad1"]
}